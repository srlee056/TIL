{
  "name": "DSRM",
  "action": {
    "default_popup": "popup.html"
  },
  "manifest_version": 3,
  "version": "0.1",
  "description": "동사로마 사이트를 좀 더 편하게",
  "host_permissions": [
    "https://dongsaroma.com/baseball/*"
  ],
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://dongsaroma.com/baseball/*"
      ],
      "js": [
        "jquery-3.7.1.min.js",
        "myscript.js"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "service-worker.js"
  },
  "commands": {
    "click_submit_button": {
      "suggested_key": {
        "default": "Shift+Ctrl+S",
        "mac": "Shift+Ctrl+S"
      },
      "description": "동사로마 사이트를 좀 더 편하게"
    }
  }
}